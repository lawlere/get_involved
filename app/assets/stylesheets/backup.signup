//home page 
.splash {
    background: url('/images/girl_2.png') 0 0 no-repeat;
    width: 1400px;
    height: 1000px;
    color: #fff;
    font-weight: lighter;

    .button_1 {
        position: absolute;
        left: 86px;
        padding-top:200px;

    }

    .button_1 #field {
        margin-bottom: 18px;
    }

    .content {
        position: absolute;
        top: 280px;
        left: 88px;
        font-family: 'Droid Sans', sans-serif;
        font-size: 20px;
        font-weight: lighter;
    }

    .logo {
        padding-top: 120px;
        padding-left:85px;
    }

    .logo img {
        width: 450px;
        height: 13  0px;
    }

    .button_1 button {
        background-color: #ba1a63;
        cursor: pointer;
        display: inline-block;
        vertical-align: top;

        -webkit-border-radius: 8px;
        -moz-border-radius: 8px;
        -o-border-radius: 8px;
        border-radius: 8px;

        -webkit-box-shadow: 2px 2px 5px #888;
        -moz-box-shadow: 2px 2px 5px #888;
        -o-box-shadow: 2px 2px 5px #888;
        box-shadow: 2px 2px 5px #888;
  
        -webkit-transition: all 0.5s ease-in-out;
        -moz-transition: all 0.5s ease-in-out;
        -o-transition: all 0.5s ease-in-out;
        transition: all 0.5s ease-in-out;
  
        color: #202020;
        font-family:'Droid Sans', sans-serif;
        font-size: 18px;
        font-weight:bold;
        height: 35px;
        width: 155px;

    }
    

    .button_1 form #field {
        height: 35px;
        width: 148px;
        opacity:0.6;

        -webkit-border-radius: 8px;
        -moz-border-radius: 8px;
        -o-border-radius: 8px;
        border-radius: 8px;   
    }

}
.splash .photo_credit h1 {
    font-size: 10px;
    margin-top: 550px;
    margin-left: 1150px;
}    
