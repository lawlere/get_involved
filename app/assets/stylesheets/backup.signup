 //top nav bar 

.top_nav {
    width: 100%;
    height: 50px;
    font-family:'Bitter', serif;
    font-size: 16px;
    font-color:#fff;
    background-color:#404041;
}

    #navbar  {
        margin: 0;
        padding: 0;
        height: 1em; 
        margin-left: 50px;
    }
    
    #navbar  li {
        list-style: none;
        float: left; 
        margin-right: 4px;
        margin-left: 75px;
        margin-top: 13px;
    }

    #navbar  li a {
        display: block;
        padding: 3px 8px;
        background-color: inherit;
        color:  #fff ;
    #navbar  li ul {
        display: none; 
        width: 10em; /* Width to help Opera out */
        background-color: inherit;
    }

    #navbar  li:hover ul {
        display: block;
        position: absolute;
        margin: 0; 
    }
    
    #navbar  li:hover li {
        float: none; 
        margin-top: 1px;
    }
    
    #navbar  li:hover li a {
        background-color: #404041;
        border-bottom: 1px solid  #fff ;
        color: #fff;
    }

    #navbar  li li a:hover {
        font-size:110%;
        color: #ba1a63;
    }     text-decoration: none; 
    }

   

